!!!
%html
	%head
		%title= yield(:title) || "dish.fm"
		%meta{"http-equiv"=>"Content-Type", :content=>"text/html; charset=utf-8"}
		= stylesheet_link_tag :all
		= javascript_include_tag :defaults
		= javascript_include_tag 'http://maps.googleapis.com/maps/api/js?sensor=false'
		= javascript_include_tag 'jquery.rating'
		= javascript_include_tag 'jquery.masonry.min'
		= stylesheet_link_tag 'jquery-ui-1.8.16.custom'
		= stylesheet_link_tag 'jquery.rating'	
		= csrf_meta_tag
	%body
		=render '/shared/header'
		#popup_layer.scroll.hide_overflow
		#container
			#wrapper
				= yield